#: ../properties.c:1253 ../properties.c:5113
msgid   "1"
msgstr  "1"

#: ../properties.c:1254 ../properties.c:5114
msgid   "2"
msgstr  "2"

#: ../properties.c:1255 ../properties.c:5115
msgid   "3"
msgstr  "3"

#: ../properties.c:1256 ../properties.c:5116
msgid   "4"
msgstr  "4"

